<div class="content">
  <h1 align="center">User list</h1>
  <div class="container">
    <table class="table">
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Email</th>
        <th>Username</th>
        <th>Password</th>
        <th></th>
      </tr>

      <% @user.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.username %></td>
          <td><%= user.password %></td>
          <td><%= link_to 'Destroy', user_path(user),
                          method: :delete,
                          data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>